[core]
	editor = /usr/bin/nvim
[init]
	defaultBranch = main
[alias]
    s = status
    l = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%C(dim white) %an%C(reset)%n''%C(white)%s%C(reset) ' --all

